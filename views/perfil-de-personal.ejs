                    <div class="card">
                        <div class="card-header d-flex justify-content-between">
                            <div class="header-title">
                                <h4 class="card-title">Historial de Cambios del Docente</h4>
                            </div>
                        </div>
                        <div class="card-body">
                            <!-- Contenedor con overflow habilitado -->
                            <div class="overflow-auto" style="max-height: 600px;">
                                <div
                                    class="iq-timeline0 m-0 d-flex align-items-center justify-content-between position-relative">
                                    <ul class="list-inline p-0 m-0">
                                        <!-- Cambio 1: Registro del Docente -->
                                        <% Object.entries(historiales).forEach(([tabla, datos])=> { %>
                                            <% if (datos.length> 0) { %>
                                                <div class="historial-section">
                                                    <h5 class="section-title ms-5 mb-3 badge bg-primary"
                                                        style="font-size: 0.9rem; padding: 10px;">
                                                        <%= tabla %>
                                                    </h5>
                                                    <ul class="list-inline p-0 m-0">
                                                        <% datos.forEach(dato=> { %>
                                                            <li>
                                                                <div
                                                                    class="timeline-dots timeline-dot1 border-primary text-primary">
                                                                </div>
