                 </div>
                 <div class="card-body" style="height: 700px; overflow: hidden; padding-bottom: 3rem;">
                   <!-- Contenedor para los pendientes -->
                   <div class="overflow-auto" style="max-height: 600px;">
                     <% pendientes.forEach(pendiente => { %>
                       <div class="mb-2 d-flex profile-media align-items-top">
                         <div class="mt-1 profile-dots-pills border-primary"></div>
                         <div class="ms-4">
                           <h6 class="mb-1"><%= pendiente.tramite %></h6>
                           <span class="mb-0"><%= new Date(pendiente.fecha).toLocaleDateString('es-MX', {
                              weekday: 'short',
                              year: 'numeric',
                              month: 'long',
                              day: 'numeric'
                            }) %> <%= pendiente.estatus %>
                            </span>
                         </div>
                       </div>
                     <% }) %>
